msgctxt "ST_TextsToLocalize,0000000123"
msgid "Doubles Damage on impact"
msgstr "При попадании удваивает урон"

#. Key:	0000000124
#. SourceLocation:	/Game/Ui/Localization/ST_TextsToLocalize.ST_TextsToLocalize
#: /Game/Ui/Localization/ST_TextsToLocalize.ST_TextsToLocalize
msgctxt "ST_TextsToLocalize,0000000124"
